{
  "name": "time-sherpa",
  "version": "1.0.0",
  "description": "AI-powered Google Calendar analysis tool for leaders and executives",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "build": "npm run client:build && npm run server:build",
    "start": "node dist/server.js",
    "server:dev": "NODE_ENV=development nodemon",
    "server:build": "tsc -p tsconfig.server.json",
    "client:dev": "vite",
    "client:build": "vite build",
    "client:preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "azure:build": "npm ci && npm run build",
    "azure:start": "npm start"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@tailwindcss/postcss": "^4.1.8",
    "@tailwindcss/vite": "^4.1.8",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "googleapis": "^160.0.0",
    "helmet": "^8.1.0",
    "node-cron": "^4.1.0",
    "nodemailer": "^7.0.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "recharts": "^2.15.3",
    "session-file-store": "^1.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.0",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/react": "^19.1.7",
    "@types/react-dom": "^19.1.6",
    "@types/session-file-store": "^1.2.5",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "eslint": "^9.28.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.2.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.4",
    "supertest": "^7.1.1",
    "tailwindcss": "^4.1.8",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "keywords": [
    "calendar",
    "productivity",
    "time-management",
    "google-calendar",
    "ai",
    "analytics"
  ],
  "author": "TimeSherpa Team",
  "license": "MIT"
}
